{
  "version": 2,
  "builds": [
    {
      "src": "src/server-bulletproof.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/api/sitemap.xml.js"
    },
    {
      "src": "/moments/([a-z0-9-]+)",
      "dest": "/api/moments/[slug].js?slug=$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/webhook",
      "dest": "/src/server-bulletproof.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/server-bulletproof.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server-bulletproof.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["cpt1"]
}