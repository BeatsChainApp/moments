{
  "version": 2,
  "builds": [
    {
      "src": "src/server-bulletproof.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/src/server-bulletproof.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/server-bulletproof.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server-bulletproof.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["cpt1"]
}